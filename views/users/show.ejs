<div class="row">
    <h1>User data for: <%= user.fullName %></h1>
    <hr>
     <table class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>ZipCode</th>
            </tr>
        </thead>
        <tbody>
            
            <td><%= user.fullName %></td>
            <td><%= user.email %></td>
            <td><%= user.zipCode %></td>

        </tbody>
    </table>
    <% if (user.subscribedAccount) { %>
        <h4 class="="center"> This user has a 
            <a href="<%=`/subscribers/${user.subscribedAccount}` %>">subscribed account</a>
        </h4>
    <% } %>
</div>